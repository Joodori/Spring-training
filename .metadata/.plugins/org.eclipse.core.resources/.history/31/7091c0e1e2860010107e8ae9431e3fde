package customer;

public class Customer {
	String adress;
	String name;
	
	Customer(String address, String name) {
		this.adress = address;
		this.name = name;
	}
	
	public static String getAdress() {
		return adress;
	}
	public void setAdress(String adress) {
		this.adress = adress;
	}
	public static String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
}
