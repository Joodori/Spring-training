<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper-addrbook">
	<select id="getDBList" resultType="addrbook.AddrBookVO">
		SELECT ab_id abId
		, ab_name abName
		, ab_email abEmail
		, ab_comdept abComdept
		, ab_birth abBirth
		from
		addrbook
	</select>
	<insert id="insertDB" parameterType="addrbook.AddrBookVO">
		insert into addrbook
		( ab_name
		, ab_email
		, ab_comdept
		, ab_birth
		, ab_tel
		, ab_memo )
		values(#{abName}, ${abEmail}, ${abComdept}, ${abBirth}, ${abTel}, ${abMemo});

	</insert>
</mapper>