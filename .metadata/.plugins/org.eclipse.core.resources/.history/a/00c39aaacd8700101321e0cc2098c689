package myspring;

public class Emp {
	int employeeId, deptId, salary;
	String firstName, lastName, hireDate, email;
	String departmentName;
	
	@Override
	public String toString() {
		return "Emp [empId=" + employeeId + ", deptId=" + deptId + ", salary=" + salary + ", firstName=" + firstName
				+ ", lastName=" + lastName + ", hireDate=" + hireDate + ", email=" + email + "]";
	}
}
