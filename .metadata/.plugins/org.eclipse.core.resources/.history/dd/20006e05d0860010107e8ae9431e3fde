<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper-addrbook">
	<select id="getDBList" resultType="addrbook.AddrBookVO">
		SELECT ab_id abId
		, ab_name abName
		, ab_email abEmail
		, ab_comdept abComdept
		, ab_birth abBirth
		from addrbook
	</select>
</mapper>